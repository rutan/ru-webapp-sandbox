{
  "name": "ru-webapp-sandbox",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "vite preview",
    "dev": "vite",
    "build": "run-s build:*",
    "build:vite": "cross-env NODE_ENV=production vite build",
    "lint": "run-s lint:*",
    "lint:biome": "biome check ./",
    "lint:tsc": "tsc --noEmit",
    "format": "biome check --write ./",
    "deploy": "wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "hono": "^4.9.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@cloudflare/vite-plugin": "^1.12.3",
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "cross-env": "^10.0.0",
    "npm-run-all2": "^8.0.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "wrangler": "^4.34.0"
  },
  "packageManager": "pnpm@10.15.1"
}
